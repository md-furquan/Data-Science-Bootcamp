   "source": [
    "i = int(input())\n",
    "j = int(input())\n",
    "init_index = min(i,j)+1\n",
    "final_index = max(i,j)\n",
    "step=2\n",
    "\n",
    "if init_index % 2 == 0:\n",
    "    init_index += 1\n",
    "\n",
    "odd_sum = 0\n",
    "for i in range(init_index,final_index, step):\n",
    "    odd_sum += i\n",
    "print(odd_sum)"
   ]
  }
