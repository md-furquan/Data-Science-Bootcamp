   "source": [
    "#Bhaskara's Formula\n",
    "import math\n",
    "A,B,C = map(float,input().split())\n",
    "D = (B**2)-(4*A*C)\n",
    "if(D <0 or A==0):\n",
    "    print(\"Impossivel calcular\")\n",
    "else:\n",
    "    D=math.sqrt(D)\n",
    "    R1 = (-B+D)/(2*A)\n",
    "    R2 = (-B-D)/(2*A)\n",
    "    print(f'R1 = {R1:0.5f}\\nR2 = {R2:0.5f}')"
   ]
  }
