   ],
   "source": [
    "import numpy as np\n",
    "a = np.random.random((25)).reshape(5,5)\n",
    "row_sums = a.sum(axis=1)\n",
    "new_matrix = a / row_sums[:, np.newaxis]\n",
    "print(new_matrix)\n",
    "\n",
    "#Broadcasting is really good for this"
   ]
  }
