   ],
   "source": [
    "import numpy as np\n",
    "number = [1,2,0,0,4,0]\n",
    "vector = np.array(number)\n",
    "size   = len(vector)\n",
    "for i in range(size):\n",
    "    if vector[i]==0:\n",
    "        print(i+1)"
